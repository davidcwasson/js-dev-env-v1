{
  "name": "js-dev-env-v1",
  "version": "1.0.0",
  "description": "JavaScript Development Environment",
  "main": "app.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint app.js",
    "lint-fix": "./node_modules/.bin/eslint app.js --fix",
    "start": "DEBUG=app nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidcwasson/js-dev-env-v1.git"
  },
  "author": "David Wasson",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/davidcwasson/js-dev-env-v1/issues"
  },
  "homepage": "https://github.com/davidcwasson/js-dev-env-v1#readme",
  "dependencies": {
    "bootstrap": "4.5.2",
    "chalk": "4.1.0",
    "debug": "4.1.1",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "jquery": "3.5.1",
    "morgan": "1.10.0",
    "nodemon": "2.0.4",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "eslint": "7.9.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4000
    }
  }
}
