<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/header'); %>
</head>

<body>

  <%- include('../partials/nav'); %>

  <!-- Page Header -->
  <header class="masthead" style="background-image: url('../img/about-bg.jpg')">
    <div class="overlay"></div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <div class="post-heading">
            <h1><%=todo.title%></h1>
            <h2 class="subheading">Genre: <%=todo.genre%> - Completed: <%=todo.completed%></h2>
            <span class="meta">Posted by
              <a href="#"><%=todo.author%></a>
              repeat <%=todo.repeat%></span>
          </div>
        </div>
      </div>
    </div>
  </header>

  <!-- Post Content -->
  <article>
    <div class="container">
      <div class="row">
        <div class="col-lg-8 col-md-10 mx-auto">
          <p>Coming soon...</p>
        </div>
      </div>
    </div>
  </article>

  <hr>

  <%- include('../partials/footer'); %>

</body>

</html>
